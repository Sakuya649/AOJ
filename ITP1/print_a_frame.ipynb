      "source": [
        "while True:\n",
        "  h, w = map(int, input().split())\n",
        "  if h == 0:break\n",
        "  for i in range(h):\n",
        "    if i == 0 or i == h-1:\n",
        "      print(\"#\"*w)\n",
        "    else:\n",
        "      print(\"#\" + \".\"*(w-2) + \"#\")\n",
        "  print()"
      ],
