      "source": [
        "while True:\n",
        "  h, w = map(int, input().split())\n",
        "  if h == 0 and w == 0:\n",
        "    break\n",
        "  else:\n",
        "    for i in range(h):\n",
        "      print(\"#\"*w)\n",
        "  print(\"\")"
