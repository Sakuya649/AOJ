      "source": [
        "while True:\n",
        "  a, op, b = input().split()\n",
        "  a, b = int(a), int(b)\n",
        "  if op == \"?\":\n",
        "    break\n",
        "  elif op == \"+\":\n",
        "    print(a + b)\n",
        "  elif op == \"-\":\n",
        "    print(a - b )\n",
        "  elif op == \"*\":\n",
        "    print(a * b)\n",
        "  else:\n",
        "    print(a // b)\n"
      ],
      "execution_count": 6,
      "outputs": [
        {
          "name": "stdout",
          "output_type": "stream",
