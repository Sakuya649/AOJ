      "source": [
        "n = int(input())\n",
        "num_list = []\n",
        "for i in range(3, n+1):\n",
        "  if i % 3 == 0 or \"3\" in str(i):\n",
        "    num_list.append(i)\n",
        "print(\" \", end=\"\")\n",
        "print(*num_list)"
