      "source": [
        "n, m = map(int, input().split())\n",
        "a_list = [[int(a) for a in input().split()] for _ in range(n)]\n",
        "b_list = [int(input()) for b in range(m)]\n",
        "for a in a_list:\n",
        "  print(sum(a[i]*b_list[i] for i in range(m)))"
      ],
