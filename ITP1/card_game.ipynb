      "source": [
        "n = int(input())\n",
        "t_score, h_score = 0, 0\n",
        "for _ in range(n):\n",
        "  t, h = input().split()\n",
        "  if t < h:\n",
        "    h_score += 3\n",
        "  elif t > h:\n",
        "    t_score += 3\n",
        "  else:\n",
        "    t_score += 1\n",
        "    h_score += 1\n",
        "print(t_score, h_score)"
      ]
    }
  ]
