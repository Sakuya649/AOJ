      "source": [
        "s = input()\n",
        "for _ in range(int(input())):\n",
        "  q = input().split()\n",
        "  a, b = map(int,q[1:3])\n",
        "  b += 1\n",
        "  if q[0] == \"print\":\n",
        "    print(s[a:b])\n",
        "  elif q[0] == \"reverse\":\n",
        "    s = s[:a] + s[a:b][::-1] + s[b:]\n",
        "  else:\n",
        "    s = s[:a] + q[3] + s[b:]"
      ]
    }
  ]
